{
  "name": "@shieldmaidengames/multipipe",
  "version": "0.0.0",
  "description": "Game and server for multipipe",
  "license": "ISC",
  "author": "polerin <polerin@eclexia.net>",
  "type": "module",
  "exports": {
    "./tsconfig.json": "./tsconfig.json"
  },
  "scripts": {
    "build": "nx run-many -t build --verbose",
    "clean": "nx run-many -t clean && nx reset",
    "start": "npm run build && npm -w @shieldmaidengames/multipipe-server run start",
    "start:dev": "npm run build && concurrently 'npm run watch' 'npm -w @shieldmaidengames/multipipe-server run start:dev'",
    "watch": "nx watch --all -- nx run \\$NX_PROJECT_NAME:build --verbose",
    "build:frontend": "nx build @themaven-net/commerce-standalone-frontend",
    "build:api": "nx build @themaven-net/commerce-api-server",
    "build:tools": "nx build @themaven-net/commerce-tools",
    "build:internal": "nx build @themaven-net/commerce-internal",
    "build:cms-integration": "nx build @themaven-net/cms-integration",
    "build:shared": "nx build @themaven-net/commerce-shared",
    "test:eslint": "nx run-many -t test:eslint",
    "test:madge": "nx run-many -t test:madge",
    "test:unit": "nx run-many -t test",
    "test:tsc": "nx run-many -t test:tsc",
    "test": "npm clean && npm build && concurrently 'npm test:tsc' 'npm test:unit' 'npm test:eslint'",
    "graph": "npm nx graph"
  },
  "workspaces": [
    "apps/*",
    "lib/*"
  ],
  "dependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@rollup/plugin-url": "^8.0.2",
    "concurrently": "^9.1.1",
    "lerna": "^8.1.9",
    "nx": "^20.3.0",
    "rollup": "^4.29.1",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@parcel/transformer-typescript-types": "^2.13.3"
  }
}
