{
  "name": "@shieldmaidengames/multipipe-frontend",
  "version": "0.0.0",
  "description": "multipipe game server",
  "license": "ISC",
  "type": "module",
  "author": "polerin (polerin@eclexia.net)",
  "scripts": {
    "build": "concurrently 'yarn build:index' 'yarn build:multipipe'",
    "build:index": "parcel build --no-cache ./markup/index.html --dist-dir ./dist --public-url /multipipe",
    "build:multipipe": "parcel build --no-cache ./markup/game_container.html --dist-dir ./dist --public-url /multipipe",
    "test": "yarn run build && npx playwright test"
  },
  "dependencies": {
    "@lit/context": "^1.1.3",
    "@shieldmaidengames/multipipe-display-components": "0.0.0",
    "@shieldmaidengames/multipipe-internal-shared": "0.0.0",
    "@shieldmaidengames/multipipe-shared": "0.0.0",
    "ditox": "^3.0.0",
    "lit": "^3.2.1"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.13.3",
    "@parcel/packager-ts": "^2.13.3",
    "@parcel/transformer-typescript-tsc": "^2.13.3",
    "@playwright/test": "^1.49.0",
    "@types/node": "^22.10.0",
    "parcel": "2.13.3",
    "typescript": "5.7.2"
  }
}
